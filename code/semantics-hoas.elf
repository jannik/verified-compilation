eval : exp -> exp -> type.

eval/num : eval (num N) (num N).

eval/lam : eval (lam E1) (lam E1).

eval/app : eval (app E1 E2) C
            <- eval E1 (lam E0)
            <- eval E2 C2
            <- eval (E0 C2) C.

eval/suc : eval (suc E1) (num (s N))
            <- eval E1 (num N).

%worlds () (eval _ _).

