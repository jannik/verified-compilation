% The source language -- uses higher-order abstract syntax.
exp : type. %name exp E x.

num : nat -> exp.
lam : (exp -> exp) -> exp.
app : exp -> exp -> exp.

%block bl-exp : block {e : exp}.
%worlds (bl-exp) (exp).



% Canonical forms.
canon : exp -> type. %name canon DC.

canon/num : canon (num N).

canon/lam : canon (lam E).
