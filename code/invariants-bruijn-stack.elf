cor-bs : value -> svalue -> type. %name cor-bs DC.
comp-bs : store -> sstore -> type. %name comp-bs DD.

cor-bs/num : cor-bs (vnum N) (wnum N).

cor-bs/fun : cor-bs (vclos Alph B) (wclos Bet P)
              <- comp-bs Alph Bet
              <- trans-bs B P.

comp-bs/nil : comp-bs store/nil sstore/nil.

comp-bs/cons : comp-bs (store/cons Alph V) (sstore/cons Bet W)
                <- comp-bs Alph Bet
                <- cor-bs V W.
